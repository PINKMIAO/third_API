<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.meorient.avaya.mapper.PhoneLogMapper">
    
    <resultMap id="getPhoneLogMsg" type="phoneAttriLog">
        <result column="phone_num" property="phoneNum"/>
        <result column="add_zero" property="addZero"/>
        <result column="create_time" property="createTime"/>
        <result column="req_time" property="reqTime"/>
        <result column="request_log" property="requestLog"/>
        <result column="response_log" property="responseLog"/>
        <result column="run_time" property="runTime"/>
    </resultMap>
    
    <select id="getAllMsg" resultMap="getPhoneLogMsg">
        select * from phone_attri_log;
    </select>
    <insert id="insertPhoneMsgLog" parameterType="map">
        insert into phone_attri_log (phone_num, add_zero, create_time, req_time, request_log, response_log, run_time)
        value (#{phoneNum}, #{addZero}, #{createTime}, #{reqTime}, #{requestLog},#{responseLog}, #{runTime});
    </insert>
</mapper>